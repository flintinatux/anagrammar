{
  "name": "anagrammar",
  "author": "Scott McCormack",
  "description": "Description",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/flintinatux/anagrammar"
  },
  "scripts": {
    "build": "brunch build --production",
    "clean": "rm -rf public/*",
    "deploy": "cd public/ && git checkout gh-pages && git add --all && git commit -m \"Deploy\" && git push origin gh-pages && cd ..",
    "prebuild": "npm run clean",
    "predeploy": "npm run build",
    "start": "brunch watch --server"
  },
  "devDependencies": {
    "auto-reload-brunch": "^1.7.6",
    "autoprefixer-brunch": "^1.8.4",
    "clean-css-brunch": "^1.7.1",
    "coffee-script-brunch": "^1.8.1",
    "css-brunch": "^1.7.0",
    "digest-brunch": "^1.4.0",
    "javascript-brunch": "^1.7.1",
    "less-brunch": "^1.8.1",
    "uglify-js-brunch": "^1.7.7"
  }
}
